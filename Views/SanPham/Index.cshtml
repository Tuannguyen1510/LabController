
@{
    ViewData["Title"] = "Index";
    var sp = ViewBag.SanPhams;
}


<div class="row">
    <div class="col-4">
        <h5>Danh Mục</h5>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Quần Áo</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row">Túi Xách</th>
                  
                </tr>
                <tr>
                    <th scope="row">Đồng Hồ</th>
                  
                </tr>
                <tr>
                    <th scope="row">Ti Vi</th>
                </tr>
                <tr>
                    <th scope="row">Tủ lạnh</th>
                </tr>
                <tr>
                    <th scope="row">Máy Bơm</th>
                </tr>
                <tr>
                    <th scope="row">Quạt điện</th>
                </tr>
                <tr>
                    <th scope="row">Lò sưởi</th>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="col-8">
        <h5>Danh sách sản phẩm</h5>

        <div class="row">

            @foreach(var item in sp)
            {
                <div class="col-4">
                    <div class="card" style="width: 18rem;">
                        <img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title">@item.Name</h5>
                            <p class="card-text">@item.Price</p>
                            <a asp-route="details" asp-route-id="@item.Id" class="btn btn-danger ">Chi tiết</a>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>

