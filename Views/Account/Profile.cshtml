
@{
    ViewData["Title"] = "Profile";
    var accounts = ViewBag.a;
}

<div class="col-3 bg-danger">
    <div class="card" style="width: 18rem;">
        <img src="@accounts.Avatar" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">@accounts.Id</h5>
            <p class="card-text">@accounts.Name</p>
            <a asp-controller="Account" asp-action="Index" class="btn btn-danger ">Tiếp</a>
        </div>
    </div>
</div>

